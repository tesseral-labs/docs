{
  "scripts": {
    "docs:custom-app:build": "esbuild --bundle ./custom-app/main.tsx --outfile=./fern/custom-app-dist/index.js ",
    "docs:preview": "fern docs dev",
    "sdk:preview": "npm-run-all -p sdk:preview:**",
    "sdk:preview:backend:node": "fern generate --preview --api backend --group node",
    "sdk:preview:backend:python": "fern generate --preview --api backend --group python",
    "sdk:preview:backend:go": "fern generate --preview --api backend --group go",
    "sdk:preview:frontend:vanilla-clientside-javascript": "fern generate --preview --api frontend --group vanilla-clientside-javascript"
  },
  "dependencies": {
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "esbuild": "^0.25.5",
    "fern-api": "^0.55.1",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
